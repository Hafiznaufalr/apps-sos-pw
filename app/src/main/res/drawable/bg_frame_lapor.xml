<?xml version="1.0" encoding="utf-8"?>
<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:tools="http://schemas.android.com/tools"
    android:width="360dp"
    android:height="60dp"
    android:viewportWidth="360"
    android:viewportHeight="60"
    >

    <group>

        <clip-path
            android:pathData="M0 0H360V60H0V0Z"
            tools:ignore="VectorRaster" />

        <group>

            <path
                android:pathData="M0 0V60H360V0"
                >

                <aapt:attr name="android:fillColor">

                    <gradient
                        android:type="linear"
                        android:startX="90"
                        android:startY="30"
                        android:endX="270"
                        android:endY="30"
                        >

                        <item
                            android:color="#FF306E"
                            android:offset="0"
                            />

                        <item
                            android:color="#FF4259"
                            android:offset="1"
                            />

                    </gradient>

                </aapt:attr>

            </path>

        </group>

    </group>

</vector>

